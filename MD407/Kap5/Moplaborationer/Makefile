.PHONY: clean All

All:
	@echo "----------Building project:[ pong2000 - Debug ]----------"
	@cd "pong2000" && "$(MAKE)" -f  "pong2000.mk" && "$(MAKE)" -f  "pong2000.mk" PostBuild
clean:
	@echo "----------Cleaning project:[ pong2000 - Debug ]----------"
	@cd "pong2000" && "$(MAKE)" -f  "pong2000.mk" clean
